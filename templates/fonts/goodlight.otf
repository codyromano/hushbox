		<script type="text/javascript">
			function VIEW_NAVIGATION_NEW(){
				var ths = this;
				ths.view_id = "view_navigation_new";
				ths.banners = [0,1,2];
				ths.banner_frames = [0,0.45,1.85,4.45,8.5,14.25,21.9,31.5,42.65,54.4,65.45,75.05,82.8,88.85,93.3,96.45,98.5,99.65,100];
				ths.framerate = 1000/(94/2);
				ths.shiftdelay = 5000;
				ths.animation;
				ths.interval;
				ths.isOn=false;
				ths.init = function(){
					ths.interval = setInterval(ths.shift, ths.shiftdelay);
				}
				ths.shift = function(){
					var animation = {
						frames:ths.banner_frames.slice(),
						update:function() {
							$("#banners_off_list").style.marginTop = map(animation.frames.shift(), 0, 100, 0, -56)+"px";
							if( !animation.frames.length ){
								animation.complete();
								animation=null;
							}
						},
						complete:function() {
							clearInterval(animation.interval);
							$("#banners_off_list").insertBefore(document.getElementById("banners_off_list_item"+ths.banners[0]),null);
							$("#banners_on_list").insertBefore(document.getElementById("banners_on_list_item"+ths.banners[0]),null);
							$("#banners_off_list").style.marginTop = "0px";
							ths.banners.push(ths.banners.shift());
							animation=null;
						}
					}
					animation.interval = setInterval(animation.update, ths.framerate);
					animation.update();

					ths.animation = animation;
				}
				ths.on = function(){

					if(!ths.isOn)
					{
						ths.isOn=true;

						var lists = $("#view_navigation > ul > li");

						//TODO
						//if (navigator.appName != 'Microsoft Internet Explorer')
						//{
							for(var i0=0; i0<lists.length; i0++){
								var list = lists[i0];
								var items = $("#"+list.id+" ul li");
								for(var i1=0; i1<items.length; i1++){
									proto.tween(items[i1], {prop:"opacity", start:0, end:0.99, duration:12, delay:i1*3})
								}
							}
						//}

						// Stop the banners switching
						clearInterval(ths.interval);

						//Open elements
						$("#banners_off").style.display="none";
						$("#banners_on").style.display="block";
						$("#view_navigation").className = "view_navigation on";
						$("#close").style.display="block";
					}
				}

				ths.off = function(){

					if(ths.isOn){

						ths.isOn=false;

						//Open elements
						$("#banners_off").style.display="block";
						$("#banners_on").style.display="none";
						$("#view_navigation").className = "view_navigation";
						$("#close").style.display="none";

						ths.interval = setInterval(ths.shift, ths.shiftdelay);
					}
				}

				ths.toggle = function(){
					(ths.isOn) ? ths.off() : ths.on();
				}
			}
			var view_navigation_new = new VIEW_NAVIGATION_NEW();		// view-instance
			view_navigation_new.init();

			//view_navigation_new.on();

		</script>

		
					
